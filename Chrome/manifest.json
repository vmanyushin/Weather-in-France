{
  "manifest_version": 2,
  "name": "Weather in France",
  "description": "http://www.site.com",
  "version": "1.2",
  "icons": {
    "128": "128.png"
  },

  "browser_action": {
    "default_title": "Weather",
    "default_icon" : "128.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "unlimitedStorage",
    "notifications",
    "geolocation",
    "storage"
  ]
}